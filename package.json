{
  "name": "@nuxt-themes/alpine",
  "version": "1.6.2",
  "type": "module",
  "exports": "./nuxt.config.ts",
  "packageManager": "pnpm@8.5.1",
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "types",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .starters/default",
    "dev": "nuxi dev .starters/default",
    "build": "nuxi build .starters/default",
    "generate": "nuxi generate .starters/default",
    "preview": "nuxi preview .starters/default",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt-themes/elements": "^1.0.6",
    "@nuxt-themes/tokens": "^1.9.1",
    "@nuxt-themes/typography": "^0.11.0",
    "@nuxt/content": "^2.7.0",
    "@nuxthq/studio": "^0.13.3",
    "@vueuse/core": "^10.2.1",
    "ufo": "^1.1.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/plausible": "^0.2.1",
    "@types/node": "^20.4.0",
    "eslint": "^8.44.0",
    "nuxt": "^3.6.2",
    "release-it": "^16.0.0",
    "typescript": "^5.1.6",
    "vite-plugin-inspect": "^0.7.32",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vite": "*"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios",
        "vite",
        "@nuxt/vite-builder"
      ]
    }
  },
  "license": "MIT",
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
